{
  "views": {
    "auth_ap_by_ssid": {
        "map": "function (doc, meta) {if( doc.apid) emit([doc.ssid], {\"ssid\":doc.ssid, \"building\": doc.buid,\"floor\":doc.floor}); }"
     },

    "auth_ap_by_buid_floor": {
    	"map": "function (doc, meta) {if( doc.apid) emit([doc.buid, doc.floor], {\"ssid\":doc.ssid});}"
    },

    "auth_ap_all": {
    	"map": "function (doc, meta) {if( doc.apid) emit(doc.apid, {\"ssid\":doc.ssid, \"building\": doc.buid,\"floor\":doc.floor}); }"
    }
  }
}
